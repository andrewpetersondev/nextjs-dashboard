{
	"extends": "../tsconfig.base.json",
	"compilerOptions": {
		"allowJs": true,
		"noEmit": true,
		"esModuleInterop": true,
		"isolatedModules": true,
		"baseUrl": "../",
		"paths": {
			"@/*": ["./*"]
		}
	},
	"include": [
		"**/*.ts",
		"**/*.tsx",
		"**/*.cy.ts",
		"**/*.cy.tsx",
		"cypress.d.ts",
		"../cypress.config.ts"
	]
}
