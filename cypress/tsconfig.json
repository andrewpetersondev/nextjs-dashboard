{
	"compilerOptions": {
		"allowJs": true,
		"checkJs": false,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"incremental": false,
		"isolatedModules": true,
		"jsx": "preserve",
		"lib": ["es5", "dom"],
		"module": "esnext",
		"moduleResolution": "node",
		"noEmit": true,
		"noImplicitAny": true,
		"plugins": [],
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true,
		"strictBindCallApply": true,
		"strictFunctionTypes": true,
		"strictNullChecks": true,
		"target": "es5",
		"types": ["cypress", "node"]
	},
	"extends": "../tsconfig.json",
	"include": ["./cypress.d.ts", "**/*.ts", "**/*.tsx"]
}
