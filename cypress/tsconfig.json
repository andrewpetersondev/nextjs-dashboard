{
  "extends": "../tsconfig.base.json",
  "compilerOptions": {
    "types": ["cypress", "node"],
    "allowJs": true,
    "noEmit": true,
    "esModuleInterop": true,
    "isolatedModules": true
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "**/*.cy.ts",
    "**/*.cy.tsx",
    "cypress.d.ts",
    "../cypress.config.ts"
  ]
}
