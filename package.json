{
  "name": "nextjs-dashboard",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbopack",
    "debug": "node --inspect=0.0.0.0:9229 node_modules/.bin/next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "create-auth-secret": "dlx auth secret",
    "reset-db": "tsx src/db/seeds/reset.ts",
    "schema-update": "drizzle-kit push --config drizzle.config.ts",
    "schema-update-initial": "drizzle-kit push:pg --config drizzle.config.ts",
    "drizzle-seed": "tsx src/db/seeds/bestSeed.ts",
    "update-drizzle-kit": "drizzle-kit up --config drizzle.config.ts",
    "cypress:open": "cypress open",
    "e2e": "start-server-and-test dev http://localhost:3000 \"cypress open --e2e\"",
    "e2e:headless": "rm -f cypress.config.js && start-server-and-test dev http://localhost:3000 \"cypress run --e2e\"",
    "component": "cypress open --component",
    "component:headless": "rm -f cypress.config.js && cypress run --component",
    "docker:build": "docker compose -f compose.dev.yaml build",
    "docker:up": "docker compose -f compose.dev.yaml up --build",
    "docker:down": "docker compose -f compose.dev.yaml down",
    "docker:logs": "docker compose -f compose.dev.yaml logs -f",
    "docker:test": "./docker-test.sh",
    "docker:clean": "docker compose -f compose.dev.yaml down -v",
    "docker:restart": "pnpm docker:down && pnpm docker:up",
    "docker:dev": "pnpm docker:up && pnpm docker:logs",
    "docker:root": " docker exec -u root -it nextjs-dashboard-web-1 /bin/bash",
    "clean:cypress": "rm -rf cypress/**/*.js cypress/**/*.js.map",
    "pretest": "pnpm clean:cypress"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@next/env": "^15.3.1",
    "@tailwindcss/forms": "^0.5.10",
    "autoprefixer": "^10.4.21",
    "bcryptjs": "^3.0.2",
    "clsx": "^2.1.1",
    "dotenv": "^16.5.0",
    "drizzle-orm": "^0.43.1",
    "drizzle-zod": "^0.7.1",
    "jose": "^6.0.10",
    "next": "^15.3.1",
    "pg": "^8.15.6",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "server-only": "^0.0.1",
    "use-debounce": "^10.0.4",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.14.0",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.25.1",
    "@eslint/json": "^0.12.0",
    "@eslint/markdown": "^6.4.0",
    "@headlessui/react": "^2.2.2",
    "@next/eslint-plugin-next": "^15.3.1",
    "@tailwindcss/postcss": "^4.1.4",
    "@testing-library/cypress": "^10.0.3",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^22.15.3",
    "@types/pg": "^8.11.14",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "aria-query": "^5.3.2",
    "axe-core": "^4.10.3",
    "cypress": "^14.3.2",
    "cypress-axe": "^1.6.0",
    "depcheck": "^1.4.7",
    "drizzle-kit": "^0.31.0",
    "drizzle-seed": "^0.3.1",
    "eslint": "^9.25.1",
    "eslint-config-next": "^15.3.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-cypress": "^4.3.0",
    "eslint-plugin-drizzle": "^0.2.3",
    "eslint-plugin-next": "^0.0.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.0.0",
    "lightningcss": "^1.29.3",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "start-server-and-test": "^2.0.11",
    "stylelint-config-prettier": "^9.0.5",
    "tailwindcss": "^4.1.4",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.1"
  },
  "packageManager": "pnpm@10.10.0+sha512.d615db246fe70f25dcfea6d8d73dee782ce23e2245e3c4f6f888249fb568149318637dca73c2c5c8ef2a4ca0d5657fb9567188bfab47f566d1ee6ce987815c39"
}
