{
	"name": "nextjs-dashboard",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "next build --turbopack",
		"create-auth-secret": "dlx auth secret",
		"cyp:clean": "rm -rf cypress/**/*.js cypress/**/*.js.map",
		"cyp:component": "cypress open --component",
		"cyp:component:headless": "rm -f cypress.config.js && cypress run --component",
		"cyp:open": "cypress open",
		"cyp:open:testDB": "pnpm cypress open",
		"cyp:pretest": "pnpm cyp:clean",
		"cyp:test:e2e": "pnpm cyp:test:setup && pnpm cypress run",
		"cyp:test:setup": "pnpm drizzle-kit push",
		"debug": "node --inspect=0.0.0.0:9229 node_modules/.bin/next dev",
		"dev": "next dev --turbopack",
		"docker:build": "docker compose -f compose.dev.yaml build",
		"docker:clean": "docker compose -f compose.dev.yaml down -v",
		"docker:dev": "pnpm docker:up && pnpm docker:logs",
		"docker:down": "docker compose -f compose.dev.yaml down",
		"docker:logs": "docker compose -f compose.dev.yaml logs -f",
		"docker:restart": "pnpm docker:down && pnpm docker:up",
		"docker:root": " docker exec -u root -it web /bin/bash",
		"docker:test": "./docker-test.sh",
		"docker:up": "docker compose -f compose.dev.yaml up --build",
		"drizzle-reset-db": "pnpm dlx tsx src/db/seeds/reset.ts",
		"drizzle-reset-test-db": "pnpm dlx tsx src/db/seeds/reset-test-db.ts",
		"drizzle-schema-update": "pnpm drizzle-kit push --config=drizzle-dev.config.ts",
		"drizzle-schema-update-testDB": "drizzle-kit push --config=drizzle-test.config.ts",
		"drizzle-seed": "tsx src/db/seeds/bestSeed.ts --config=drizzle-dev.config.ts",
		"drizzle-seed-testDB": "tsx src/db/seeds/best-seed-testdb.ts --config=drizzle-test.config.ts",
		"drizzle-update-drizzle-kit": "pnpm drizzle-kit up --config=drizzle-dev.config.ts",
		"drizzle-update-drizzle-kit-testDB": "pnpm drizzle-kit up --config=drizzle-test.config.ts",
		"lint": "next lint",
		"reformat:project": "pnpm prettier --write .",
		"start": "node .next/standalone/server.js",
		"start:standalone": "cp -r public .next/standalone/ && cp -r .next/static .next/standalone/.next/ && node .next/standalone/server.js",
		"standalone": "rm -rf .next/ && pnpm build && pnpm start:standalone"
	},
	"dependencies": {
		"@heroicons/react": "^2.2.0",
		"@next/env": "^15.3.3",
		"@tailwindcss/forms": "^0.5.10",
		"autoprefixer": "^10.4.21",
		"bcryptjs": "^3.0.2",
		"clsx": "^2.1.1",
		"dotenv": "^16.5.0",
		"drizzle-orm": "^0.44.2",
		"drizzle-zod": "^0.8.2",
		"jose": "^6.0.11",
		"next": "15.4.0-canary.52",
		"pg": "^8.16.0",
		"react": "^19.1.0",
		"react-dom": "^19.1.0",
		"server-only": "^0.0.1",
		"use-debounce": "^10.0.5",
		"zod": "^3.25.56"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@cypress/code-coverage": "^3.14.4",
		"@eslint/eslintrc": "^3.3.1",
		"@eslint/js": "^9.28.0",
		"@eslint/json": "^0.12.0",
		"@eslint/markdown": "^6.5.0",
		"@headlessui/react": "^2.2.4",
		"@next/eslint-plugin-next": "^15.3.3",
		"@tailwindcss/postcss": "^4.1.8",
		"@testing-library/cypress": "^10.0.3",
		"@testing-library/dom": "^10.4.0",
		"@testing-library/react": "^16.3.0",
		"@types/node": "^22.15.30",
		"@types/pg": "^8.15.4",
		"@types/react": "^19.1.6",
		"@types/react-dom": "^19.1.6",
		"@typescript-eslint/eslint-plugin": "^8.33.1",
		"@typescript-eslint/parser": "^8.33.1",
		"aria-query": "^5.3.2",
		"axe-core": "^4.10.3",
		"cypress": "^14.4.1",
		"cypress-axe": "^1.6.0",
		"depcheck": "^1.4.7",
		"dotenv-cli": "^8.0.0",
		"drizzle-kit": "^0.31.1",
		"drizzle-seed": "^0.3.1",
		"eslint": "^9.28.0",
		"eslint-config-next": "^15.3.3",
		"eslint-config-prettier": "^10.1.5",
		"eslint-plugin-cypress": "^5.1.0",
		"eslint-plugin-drizzle": "^0.2.3",
		"eslint-plugin-next": "^0.0.0",
		"eslint-plugin-react-hooks": "^5.2.0",
		"globals": "^16.2.0",
		"lightningcss": "^1.30.1",
		"postcss": "^8.5.4",
		"prettier": "^3.5.3",
		"prettier-plugin-organize-imports": "^4.1.0",
		"prettier-plugin-tailwindcss": "^0.6.12",
		"stylelint-config-prettier": "^9.0.5",
		"tailwindcss": "^4.1.8",
		"tsx": "^4.19.4",
		"typescript": "^5.8.3",
		"typescript-eslint": "^8.33.1"
	},
	"packageManager": "pnpm@10.11.0+sha512.6540583f41cc5f628eb3d9773ecee802f4f9ef9923cc45b69890fb47991d4b092964694ec3a4f738a420c918a333062c8b925d312f42e4f0c263eb603551f977"
}
