  },
  "name": "nextjs-dashboard",
  "packageManager": "pnpm@10.13.1+sha512.37ebf1a5c7a30d5fabe0c5df44ee8da4c965ca0c5af3dbab28c3a1681b70a256218d05c81c9c0dcf767ef6b8551eb5b960042b9ed4300c59242336377e01cfad",
  "private": true,
  "scripts": {
    "biome-check": "biome check",
    "biome-check-write": "biome check --write",
    "biome-format": "biome format --write",
    "biome-lint": "biome lint",
    "build": "next build --turbopack",
    "build-hcp-debug": "hcp vault-secrets run -- next build --turbopack --debug",
    "clean": "pnpm cyp-clean && rm -rf .next",
    "commit-check": "pnpm cyp-clean && pnpm biome-format && pnpm lint",
    "create-auth-secret": "pnpm dlx auth secret",
    "cyp-clean": "find cypress -type f \\( -name '*.js' -o -name '*.js.map' \\) -delete && rm -f cypress.config.js",
    "cyp-component": "pnpm cyp-clean &&  hcp vault-secrets run -- cypress open --component",
    "cyp-component-headless": "hcp vault-secrets run -- cypress run --component --headless",
    "cyp-e2e-headless": "hcp vault-secrets run -- pnpm cypress run --e2e",
    "cyp-e2e-headless-auth": "hcp vault-secrets run -- pnpm cypress run --e2e --headless --spec 'cypress/e2e/auth/**/*.cy.ts'",
    "cyp-open": "hcp vault-secrets run -- pnpm cypress open",
    "cyp-test-e2e": "pnpm drizzle-test-generate && pnpm drizzle-test-migrate  && pnpm cyp-e2e-headless",
    "debug": "node --inspect=0.0.0.0:9229 node_modules/.bin/next dev",
    "dev": "next dev --turbopack",
    "dev-hcp": "hcp vault-secrets run -- pnpm dev",
    "docker-build": "docker compose -f keep/compose.dev.yaml build",
    "docker-clean": "docker compose -f keep/compose.dev.yaml down -v",
    "docker-dev": "pnpm docker-up && pnpm docker-logs",
    "docker-down": "docker compose -f keep/compose.dev.yaml down",
    "docker-logs": "docker compose -f keep/compose.dev.yaml logs -f",
    "docker-restart": "pnpm docker-down && pnpm docker-up",
    "docker-root": "docker exec -u root -it web /bin/bash",
    "docker-test": "keep/docker-test.sh",
    "docker-up": "docker compose -f keep/compose.dev.yaml up --build",
    "drizzle-dev-generate": "hcp vault-secrets run -- pnpm drizzle-kit generate --config=drizzle-dev.config.ts",
    "drizzle-dev-migrate": "hcp vault-secrets run -- pnpm drizzle-kit migrate --config=drizzle-dev.config.ts",
    "drizzle-push-dev": "hcp vault-secrets run -- pnpm drizzle-kit push --config=drizzle-dev.config.ts",
    "drizzle-push-test": "hcp vault-secrets run -- pnpm drizzle-kit push --config=drizzle-test.config.ts",
    "drizzle-reset-dev-db": "hcp vault-secrets run -- pnpm dlx tsx src/db/seeds/reset.ts",
    "drizzle-reset-test-db": "hcp vault-secrets run -- pnpm dlx tsx src/db/seeds/reset-test-db.ts",
    "drizzle-seed-dev-db": "hcp vault-secrets run -- pnpm tsx src/db/seeds/best-seed-dev-db.ts --config=drizzle-dev.config.ts",
    "drizzle-seed-test-db": "hcp vault-secrets run -- pnpm tsx src/db/seeds/best-seed-test-db.ts --config=drizzle-test.config.ts",
    "drizzle-test-generate": "hcp vault-secrets run -- pnpm drizzle-kit generate --config=drizzle-test.config.ts",
    "drizzle-test-migrate": "hcp vault-secrets run -- pnpm drizzle-kit migrate --config=drizzle-test.config.ts",
    "hcp-view-secrets-list": "hcp vault-secrets secrets list --app dashboard-app",
    "hcp-view-secrets-open": "hcp vault-secrets secrets open [your-secret-name]",
    "lint": "next lint && biome lint",
    "lint-fix": "next lint --fix && biome lint --write .",
    "standalone": "rm -rf .next/ && pnpm build && pnpm start-standalone",
    "start": "node .next/standalone/server.js",
    "start-standalone": "cp -r public .next/standalone/ && cp -r .next/static .next/standalone/.next/ && node .next/standalone/server.js",
    "type-check": "tsc --noEmit"
  },
  "type": "module",
  "version": "0.1.0"
}
