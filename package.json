{
	"name": "nextjs-dashboard",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"scripts": {
		"debug": "node --inspect=0.0.0.0:9229 node_modules/.bin/next dev",
		"dev": "next dev --turbopack",
		"dev:hcp": "hcp vault-secrets run -- pnpm dev",
		"build": "next build --turbopack",
		"lint": "next lint",
		"standalone": "rm -rf .next/ && pnpm build && pnpm start:standalone",
		"start": "node .next/standalone/server.js",
		"start:standalone": "cp -r public .next/standalone/ && cp -r .next/static .next/standalone/.next/ && node .next/standalone/server.js",
		"biome:check": "pnpm biome check --write",
		"biome:format": "pnpm biome format --write",
		"create-auth-secret": "pnpm dlx auth secret",
		"cyp:clean": "rm -rf cypress.config.js cypress/**/*.js cypress/**/*.js.map",
		"cyp:component": "pnpm cyp:clean &&  hcp vault-secrets run -- cypress open --component",
		"cyp:component:headless": "pnpm cyp:clean && cypress run --component",
		"cyp:e2e:headless": "pnpm cyp:clean && hcp vault-secrets run -- pnpm cypress run --e2e",
		"cyp:e2e:headless:auth": "pnpm cyp:clean &&  hcp vault-secrets run -- pnpm cypress run --e2e --headless --spec 'cypress/e2e/auth/**/*.cy.ts'",
		"cyp:open": "hcp vault-secrets run -- pnpm cypress open",
		"cyp:test:e2e": "pnpm drizzle:test:generate && pnpm drizzle:test:migrate  && pnpm cyp:e2e:headless",
		"drizzle:dev:generate": "hcp vault-secrets run -- pnpm drizzle-kit generate --config=drizzle-dev.config.ts",
		"drizzle:dev:migrate": "hcp vault-secrets run -- pnpm drizzle-kit migrate --config=drizzle-dev.config.ts",
		"drizzle:reset:dev_db": "hcp vault-secrets run -- pnpm dlx tsx src/db/seeds/reset.ts",
		"drizzle:reset:test_db": "hcp vault-secrets run -- pnpm dlx tsx src/db/seeds/reset-test-db.ts",
		"drizzle:seed:dev_db": "hcp vault-secrets run -- pnpm tsx src/db/seeds/best-seed-dev-db.ts --config=drizzle-dev.config.ts",
		"drizzle:seed:test_db": "hcp vault-secrets run -- pnpm tsx src/db/seeds/best-seed-test-db.ts --config=drizzle-test.config.ts",
		"drizzle:test:generate": "hcp vault-secrets run -- pnpm drizzle-kit generate --config=drizzle-test.config.ts",
		"drizzle:test:migrate": "hcp vault-secrets run -- pnpm drizzle-kit migrate --config=drizzle-test.config.ts",
		"hcp:view:secrets:list": "hcp vault-secrets secrets list --app dashboard-app",
		"hcp:view:secrets:open": "hcp vault-secrets secrets open [your-secret-name]",
		"docker:build": "docker compose -f keep/compose.dev.yaml build",
		"docker:clean": "docker compose -f keep/compose.dev.yaml down -v",
		"docker:dev": "pnpm docker:up && pnpm docker:logs",
		"docker:down": "docker compose -f keep/compose.dev.yaml down",
		"docker:logs": "docker compose -f keep/compose.dev.yaml logs -f",
		"docker:restart": "pnpm docker:down && pnpm docker:up",
		"docker:root": "docker exec -u root -it web /bin/bash",
		"docker:test": "keep/docker-test.sh",
		"docker:up": "docker compose -f keep/compose.dev.yaml up --build"
	},
	"dependencies": {
		"@heroicons/react": "^2.2.0",
		"@next/env": "^15.3.3",
		"@tailwindcss/forms": "^0.5.10",
		"autoprefixer": "^10.4.21",
		"bcryptjs": "^3.0.2",
		"clsx": "^2.1.1",
		"drizzle-orm": "^0.44.2",
		"drizzle-zod": "^0.8.2",
		"jose": "^6.0.11",
		"next": "15.4.0-canary.52",
		"pg": "^8.16.0",
		"pino": "^9.7.0",
		"react": "^19.1.0",
		"react-dom": "^19.1.0",
		"server-only": "^0.0.1",
		"use-debounce": "^10.0.5",
		"zod": "^3.25.65"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@cypress/code-coverage": "^3.14.4",
		"@eslint/eslintrc": "^3.3.1",
		"@headlessui/react": "^2.2.4",
		"@next/eslint-plugin-next": "^15.3.3",
		"@tailwindcss/postcss": "^4.1.10",
		"@testing-library/cypress": "^10.0.3",
		"@testing-library/dom": "^10.4.0",
		"@testing-library/react": "^16.3.0",
		"@types/node": "^24.0.3",
		"@types/pg": "^8.15.4",
		"@types/react": "^19.1.8",
		"@types/react-dom": "^19.1.6",
		"aria-query": "^5.3.2",
		"axe-core": "^4.10.3",
		"cypress": "^14.4.1",
		"cypress-axe": "^1.6.0",
		"depcheck": "^1.4.7",
		"drizzle-kit": "^0.31.1",
		"drizzle-seed": "^0.3.1",
		"eslint": "^9.29.0",
		"eslint-config-next": "^15.3.3",
		"eslint-plugin-react": "^7.37.5",
		"eslint-plugin-react-hooks": "^5.2.0",
		"lightningcss": "^1.30.1",
		"postcss": "^8.5.6",
		"prettier": "^3.5.3",
		"prettier-plugin-tailwindcss": "^0.6.12",
		"stylelint-config-prettier": "^9.0.5",
		"tailwindcss": "^4.1.10",
		"tsx": "^4.20.3",
		"typescript": "^5.8.3"
	},
	"engines": {
		"node": ">=24.0.0",
		"pnpm": ">=10.12.1"
	},
	"packageManager": "pnpm@10.12.1+sha512.f0dda8580f0ee9481c5c79a1d927b9164f2c478e90992ad268bbb2465a736984391d6333d2c327913578b2804af33474ca554ba29c04a8b13060a717675ae3ac"
}
