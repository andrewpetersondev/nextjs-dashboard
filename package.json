{
  "name": "nextjs-dashboard",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "NODE_OPTIONS='--inspect' next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "create-schema": "psql ./src/db/createSchema.sql",
    "create-auth-secret": "pnpx auth secret",
    "reset-db": "pnpm dlx tsx src/db/seeds/reset.ts",
    "schema-update": "npx drizzle-kit push",
    "update-package-manager": "corepack prepare pnpm@<version> --activate",
    "drizzle:seed": "pnpx tsx src/db/seeds/bestSeed.ts"
  },
  "dependencies": {
    "@auth/drizzle-adapter": "^1.7.4",
    "@auth/pg-adapter": "^1.7.4",
    "@heroicons/react": "^2.2.0",
    "@next/env": "^15.1.6",
    "@tailwindcss/forms": "^0.5.10",
    "@types/bcryptjs": "^2.4.6",
    "@typescript-eslint/eslint-plugin": "^8.21.0",
    "@typescript-eslint/parser": "^8.21.0",
    "autoprefixer": "^10.4.20",
    "bcryptjs": "^2.4.3",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.7",
    "drizzle-orm": "^0.38.4",
    "drizzle-seed": "^0.3.0",
    "drizzle-zod": "^0.6.1",
    "eslint-plugin-drizzle": "^0.2.3",
    "jose": "^5.9.6",
    "next": "15.1.6",
    "next-auth": "5.0.0-beta.25",
    "pg": "^8.13.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "server-only": "^0.0.1",
    "use-debounce": "^10.0.4",
    "uuid": "^11.0.5",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@types/bcrypt": "^5.0.2",
    "@types/node": "^22.10.9",
    "@types/pg": "^8.11.10",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "dotenv-cli": "^8.0.0",
    "drizzle-kit": "^0.30.2",
    "eslint": "^9.18.0",
    "eslint-config-next": "15.1.6",
    "eslint-config-prettier": "^10.0.1",
    "postcss": "^8.5.1",
    "prettier": "^3.4.2",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  },
  "packageManager": "pnpm@9.15.4"
}
