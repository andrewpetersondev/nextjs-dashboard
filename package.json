{
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@next/env": "15.5.2",
    "bcryptjs": "^3.0.2",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.1",
    "drizzle-orm": "^0.44.5",
    "drizzle-zod": "^0.8.3",
    "jose": "^6.1.0",
    "next": "15.5.1-canary.21",
    "pg": "^8.16.3",
    "pino": "^9.9.0",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "server-only": "^0.0.1",
    "use-debounce": "^10.0.5",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.2",
    "@tailwindcss/postcss": "^4.1.12",
    "@testing-library/cypress": "^10.0.3",
    "@types/node": "^24.3.0",
    "@types/pg": "^8.15.5",
    "@types/react": "19.1.12",
    "@types/react-dom": "19.1.9",
    "axe-core": "^4.10.3",
    "cypress": "^15.0.0",
    "cypress-axe": "^1.7.0",
    "depcheck": "^1.4.7",
    "drizzle-kit": "^0.31.4",
    "drizzle-seed": "^0.3.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.12",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=24.0.0",
    "pnpm": ">=10.12.1"
  },
  "name": "nextjs-dashboard",
  "packageManager": "pnpm@10.15.0+sha512.486ebc259d3e999a4e8691ce03b5cac4a71cbeca39372a9b762cb500cfdf0873e2cb16abe3d951b1ee2cf012503f027b98b6584e4df22524e0c7450d9ec7aa7b",
  "private": true,
  "scripts": {
    "biome-check": "biome format --write && biome check --max-diagnostics=100",
    "biome-check-write": "biome check --write",
    "biome-format": "biome format --write",
    "biome-summary": "biome check --reporter=summary",
    "build": "next build --turbopack",
    "clean": "pnpm cyp-clean && rm -rf .next",
    "create-auth-secret": "pnpm dlx auth secret",
    "cyp-e2e-headless": "pnpm cypress run --e2e",
    "cyp-e2e-headless-auth": "pnpm cypress run --e2e --headless --spec 'cypress/e2e/auth/**/*.cy.ts'",
    "cyp-open": "pnpm cypress open",
    "cyp-test-e2e": "pnpm drizzle-generate-test && pnpm drizzle-migrate-test && pnpm cyp-e2e-headless",
    "db-create": "docker exec -it dashboard-postgres psql -U {your_postgres_user} -c \"CREATE DATABASE postgres_test;\"",
    "db-reset-dev": "pnpm tsx -r dotenv/config scripts/reset-dev.ts",
    "db-reset-prod": "pnpm tsx -r dotenv/config scripts/reset-prod.ts",
    "db-reset-test": "pnpm tsx -r dotenv/config scripts/reset-test.ts",
    "db-truncate-dev": "SEED_RESET=true pnpm tsx -r dotenv/config scripts/seed-dev-db.ts",
    "db-truncate-prod": "SEED_RESET=true pnpm tsx -r dotenv/config scripts/seed-prod-db.ts",
    "db-truncate-test": "SEED_RESET=true pnpm tsx -r dotenv/config scripts/seed-test-db.ts",
    "db-truncate-test-only": "pnpm tsx -r dotenv/config scripts/truncate-test-db.ts",
    "debug": "node --inspect=0.0.0.0:9229 node_modules/.bin/next dev",
    "dev": "next dev --turbopack",
    "drizzle-push-dev": "pnpm drizzle-kit push --config=configs/drizzle-dev.config.ts",
    "drizzle-push-prod": "pnpm drizzle-kit push --config=configs/drizzle-prod.config.ts",
    "drizzle-push-test": "pnpm drizzle-kit push --config=configs/drizzle-test.config.ts",
    "hcp-view-secrets-list": "hcp vault-secrets secrets list --app dashboard-app",
    "hcp-view-secrets-open": "hcp vault-secrets secrets open [your-secret-name]",
    "seed-db-dev": "pnpm tsx -r dotenv/config scripts/seed-dev-db.ts --config=configs/drizzle-dev.config.ts",
    "seed-db-prod": "pnpm tsx -r dotenv/config scripts/seed-prod-db.ts --config=configs/drizzle-prod.config.ts",
    "seed-db-test": "pnpm tsx -r dotenv/config scripts/seed-test-db.ts --config=configs/drizzle-test.config.ts",
    "standalone": "rm -rf .next/ && pnpm build && pnpm start-standalone",
    "start": "node .next/standalone/server.js",
    "start-standalone": "cp -r public .next/standalone/ && cp -r .next/static .next/standalone/.next/ && node .next/standalone/server.js",
    "test-db-setup": "pnpm tsx -r dotenv/config scripts/setup-test-db.ts",
    "type-gen": "next typegen && tsc --noEmit"
  },
  "type": "module",
  "version": "0.1.0"
}
